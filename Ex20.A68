	ORG	$8100
MAT	DC.B	4,6,67,3,54,6,2,9,5
RG	EQU	3
CL	EQU	3
DIAG	DS.B	3
	ORG	$8000
MAIN	MOVE.L	#MAT,A0
	MOVE.L	#DIAG,A1
	CLR	D0
LOOP	CLR	D1
LOOP2	MOVE.B	(A0,D1),D4
	CMP.B	D0,D1	
	BNE	SKIP
	MOVE.B	D4,(A1)+	
SKIP	ADDQ	#1,D1
	CMP	#CL,D1
	BNE	LOOP2
	ADD	#CL,A0
	ADDQ	#1,D0
	CMP	#RG,D0
	BNE	LOOP
	END	MAIN
FINE	JMP	FINE
	
