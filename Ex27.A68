	ORG	$8100
MAIN	MOVE.L	#N-1,D4
LOOP2	MOVE.L	#VETT,A0
	MOVE.L	#VETT,A1
	MOVE.L	#N-1,D0
	
LOOP	MOVE.W (A0)+,D1
	CMP	(A0),D1
	BLE	SKIP
	MOVE	(A0),(A1)
	MOVE	D1,(A0)
SKIP	ADDQ	#2,A1	Perchè il vettore è composto da byte
	SUBQ	#1,D0
	CMP	#0,D0
	BNE	LOOP
	SUBQ	#1,D4
	CMP	#0,D4
	BNE	LOOP2
	
FINE	JMP	FINE	

	ORG	$8000
VETT	DC.W	10,7,2,3,4,11,5,1
N	EQU	8
	END	MAIN


